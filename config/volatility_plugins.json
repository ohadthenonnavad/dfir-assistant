      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.vadwalk [--pid PID]",
      "description": "Walk the VAD tree",
      "aliases": ["vadwalk"]
    },
    "windows.malfind": {
      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.malfind [--pid PID]",
      "description": "Find hidden and injected code",
      "aliases": ["malfind"]
    },
    "windows.netscan": {
      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.netscan",
      "description": "Scan for network connections and sockets",
      "aliases": ["netscan"]
    },
    "windows.netstat": {
      "version": ["vol3"],
