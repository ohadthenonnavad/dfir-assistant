{
  "metadata": {
    "version": "1.0.0",
    "description": "Volatility 2 and 3 plugin registry for command validation",
    "last_updated": "2026-01-15"
  },
  "plugins": {
    "windows.pslist": {
      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.pslist [--pid PID]",
      "description": "List processes from EPROCESS doubly-linked list",
      "aliases": ["pslist"]
    },
    "windows.psscan": {
      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.psscan",
      "description": "Scan for EPROCESS objects (finds hidden/unlinked processes)",
      "aliases": ["psscan"]
    },
    "windows.pstree": {
      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.pstree",
      "description": "Display process tree with parent-child relationships",
      "aliases": ["pstree"]
    },
    "windows.cmdline": {
      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.cmdline [--pid PID]",
      "description": "Display process command-line arguments",
      "aliases": ["cmdline"]
    },
    "windows.dlllist": {
      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.dlllist [--pid PID]",
      "description": "List loaded DLLs for each process",
      "aliases": ["dlllist"]
    },
    "windows.handles": {
      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.handles [--pid PID]",
      "description": "Display open handles for each process",
      "aliases": ["handles"]
    },
    "windows.vadinfo": {
      "version": ["vol3"],
      "syntax": "vol -f <memory_dump> windows.vadinfo [--pid PID]",
